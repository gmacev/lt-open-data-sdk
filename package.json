{
  "name": "lt-open-data-sdk",
  "version": "1.2.1",
  "description": "TypeScript SDK for the Lithuanian Open Data platform (data.gov.lt)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "lt-gen": "./dist/cli/index.js",
    "lt-data": "./dist/cli/data.js",
    "lt-open-data-sdk": "./dist/cli/data.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"try{const fs=require('fs');fs.chmodSync('dist/cli/index.js',0o755);fs.chmodSync('dist/cli/data.js',0o755);}catch{}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lt-gen": "node --loader ts-node/esm src/cli/index.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "lithuania",
    "open-data",
    "data.gov.lt",
    "spinta",
    "api-client",
    "typescript"
  ],
  "author": {
    "name": "Giedrius Maceviƒçius",
    "email": "gmacev@gmail.com",
    "url": "https://github.com/gmacev/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmacev/lt-open-data-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/gmacev/lt-open-data-sdk/issues"
  },
  "homepage": "https://github.com/gmacev/lt-open-data-sdk#readme",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "picocolors": "^1.1.1",
    "zod": "^4.3.4"
  }
}
